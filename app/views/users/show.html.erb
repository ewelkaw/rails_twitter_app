<% provide(:title, current_user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for current_user %>
        <%= current_user.name %>, <%= current_user.email %> <%= current_user.created_at %> <%= current_user.updated_at %> <%= Time.now %>
      </h1>
    </section>
  </aside>
</div>